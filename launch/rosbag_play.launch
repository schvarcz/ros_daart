<launch>
	<arg name="bagfile" default="/home/schvarcz/Desktop/2017-05-11-12-33-00.bag" />
	<param name="use_sim_time" value="True"/>
	<node pkg="rqt_bag" type="rqt_bag" name="rqt_bag" output="screen" args="$(arg bagfile) --clock"  />
	<node pkg="rviz" type="rviz" name="rviz" args="-d $(find daart)/config/config-rosbag.rviz" />
	<node pkg="tf" type="static_transform_publisher" name="odom2map" args="0 0 0 0 0 0  map odom 1000" />
	<node pkg="tf" type="static_transform_publisher" name="laser2map" args="0 0 0 0 0 0  base_link laser 1000" />
	<node pkg="tf" type="static_transform_publisher" name="imu2map" args="0 0 0 0 0 0  base_link base_imu_link 1000" />
</launch>
